{
  "name": "material-ui-remix-ts",
  "type": "module",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all --parallel start:db start:web",
    "typecheck": "tsc",
    "prepare": "husky",
    "start:web": "remix dev --manual",
    "start:db": "cockroach start-single-node --advertise-addr=127.0.0.1 --port=26257 --http-port=8080 --insecure",
    "migration:create": "mikro-orm-esm migration:create",
    "migration:up": "mikro-orm-esm migration:up"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.3",
    "@emotion/cache": "latest",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.11.5",
    "@mikro-orm/core": "^6.4.1",
    "@mikro-orm/migrations": "^6.4.1",
    "@mikro-orm/postgresql": "^6.4.1",
    "@mikro-orm/reflection": "^6.4.1",
    "@remix-run/css-bundle": "latest",
    "@remix-run/node": "2.15.2",
    "@remix-run/react": "2.15.2",
    "@remix-run/serve": "2.15.2",
    "@remix-run/server-runtime": "2.15.2",
    "isbot": "^5.1.18",
    "next-themes": "^0.4.4",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "^5.4.0",
    "store2": "^2.14.3",
    "ts-fsrs": "^3.5.7",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@mikro-orm/cli": "^6.4.1",
    "@remix-run/dev": "latest",
    "@types/node": "^22.10.2",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "latest"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  },
  "resolutions": {
    "@emotion/react": "11.14.0",
    "@emotion/utils": "1.4.2"
  },
  "overrides": {
    "@emotion/react": "11.14.0",
    "@emotion/utils": "1.4.2"
  }
}
